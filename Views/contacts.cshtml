<h1>Your Contacts<h1>
@if (@Model.Count == 0)
{
  <h3>You haven't added any contacts yet!</h3>
}
@foreach (var contact in Model)
{
  <h3><a href='/contacts/@contact.GetId()'>@contact.GetName()</a></h3>
}
<p><a href='/contacts/new'>Add a new contact</a></p>
<p><a href='/contacts/cleared'>Delete all contacts</a></p>
<p><a href="/">Go to homepage</a></p>
